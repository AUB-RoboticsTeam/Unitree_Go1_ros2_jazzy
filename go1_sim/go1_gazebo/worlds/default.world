    <!-- Cylinder 1 -->
    <model name="cylinder1">
      <pose>3 3 0.5 0 0 0</pose>
      <link name="cylinder_link">
        <inertial>
          <inertia>
            <ixx>0.1458</ixx>
            <ixy>0</ixy>
